<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".client.ui.NavigationDrawerActivity" >

    <FrameLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

    <ListView
        android:id="@+id/drawer"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/background_main_menu"
        android:divider="@color/background_main_menu"
        android:choiceMode="singleChoice"
        />

</android.support.v4.widget.DrawerLayout><!-- From: file:/home/bill/AndroidstudioProjects/MythTV-Android-Frontend/app/src/main/res/layout/activity_navigation_drawer.xml -->